import java.util.Scanner;
public class array
 {
     public void main()
     {
        Scanner sc=new Scanner(System.in);
        int ar[]=new int[10];
        int i,p=1;
        for(i=0;i<10;i++)
        {
            System.out.println("enter a number");
            ar[i]=sc.nextInt();
            if(ar[i]%2==1)
            {
                p=p*ar[i];
            }
        }
        System.out.println("product of odd number"+p);
    }
}
    
            
